{
  "name": "Everything Cat",
  "version": "1.0.0",
  "description": "An extension that turns images on website into cats",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
